	<script src="Info_tj.js"></script>
	<script class="resize" type="text/javascript">
		var currentpage = 1;
		var pagesize = 10;
		var resultdata;
		var pgzs = 0;
		$("#check").click(function () {
			check(1);
		});
		$("#push").click(function () {
			if ($("#blz_totalnum").text() == 0) {
				layer.msg("无数据可以导出!");
			} else {
				// 			url = basePath + "jsy/getJbsqexcle?'cphm'="+'{"":,"sqlx":"' + $("#jbsqsqlx").val() +'","blzt":"' + $("#jbsqblzt").val()+ '","page":1,"pageSize":100000}',window.open(url);
				url = basePath + `subsidy/findBusinessOwnerSubsidyExcel?cphm=${$('#jbsqcphm').val()}
				&shzt=${$('#jbsqblzt').val()}&sqstime=${$('#jbsqstime').val()}&sqetime=${$('#jbsqetime').val()}
				&shstime=${$('#spsjstime').val()}&shetime=${$('#spsjetime').val()}
				`
				window.open(url)
			}
		});
		$(function () {
			$('#jbsqstime').val(setformatday(new Date(new Date().getTime() - 1000 * 60 * 60 * 24)));
			$('#jbsqetime').val(setformatday(new Date()));
			check(1);
		})
		function check(obj) {
			if ($('#jbsqstime').val() == '') {
				layer.msg('申请时间不能为空！')
				return;
			}
			if ($('#jbsqetime').val() == '') {
				layer.msg('申请时间不能为空！')
				return;
			}
			$.ajax({
				type: "post",
				url: basePath + "subsidy/findCompanySubsidy",//地址、字段需改
				data: {
					'cphm': $('#jbsqcphm').val(),
					'shzt': $('#jbsqblzt').val(),
					'page': obj,
					'pageSize': 10,
					'sqstime': $('#jbsqstime').val(),
					'sqetime': $('#jbsqetime').val(),
					'shstime': $('#spsjstime').val(),
					'shetime': $('#spsjetime').val()
				},
				dataType: "json",
				success: function (data) {
					console.log(data);
					$('.table tbody').html('');
					if (data.length == 0) {
						$('.table tbody').html("<tr><td colspan='10'>查询无数据</td></tr>");
						$("#blz_totalpage").html("0");
						$("#blz_totalnum").html("0");
					} else {
						var html = '';
						for (var i = 0; i < data.length; i++) {
							html += '<tr>' +
								'<td>' + (i + 1) + '</td>' +
								'<td>' + data[i].CPHM + '</td>' +
								'<td>' + data[i].SQSJ + '</td>' +
								'<td>' + data[i].SHSJ + '</td>' +
								'<td>' + (data[i].SHZT == '0' ? '审核通过' : data[i].SHZT == '1' ? '审核不通过' : data[i].SHZT == '2' ? '审核中' : '') + '</td>' +
								'<td>' + data[i].SHBZ + '</td>' +
								'</tr>';
						}
						$('.table tbody').append(html);
						$('.cktab').on('click', function () {
							var i = $(this).attr('lx')
							window.location.href = "ddfz.html?bh=" + data[i].SQBH;
						})
						if (data.length > 0) {
							pgzs = parseInt((data[0].COUNT - 1) / pagesize) + 1;
							$("#blz_totalpage").html(pgzs);
							$("#blz_totalnum").html(data[0].COUNT);
						} else {
							$("#blz_totalpage").html("0");
							$("#blz_totalnum").html("0");
						}
						$("#blz_currentpage").html(currentpage);
					}
				},
				error: function () {
				}
			});
		}




		//首页
		function gopage() {
			currentpage = 1;
			check(currentpage);
		}
		//上一页
		function goxyy() {
			currentpage++;
			if (currentpage > pgzs) {
				layer.msg("已到末页");
				currentpage = currentpage - 1;
				return;
			}
			check(currentpage);
		}
		//下一页
		function gosyy() {
			currentpage--;
			if (currentpage <= 0) {
				layer.msg("已到首页");
				currentpage = currentpage + 1;
				return;
			}
			check(currentpage);
		}
		//末页
		function goend() {
			currentpage = pgzs;
			check(currentpage);
		}
		//跳页
		function tzym() {
			if ($('#ym').val() != "") {
				if (pgzs >= parseInt($('#ym').val())) {
					currentpage = $('#ym').val();
					check(currentpage);
				} else {
					hint_info("ym", "数字过大", "3");
				}
			}
		}
		$('#ryczqcjbsq').on('click', function () {

			window.open('qyzxbzsqForm.html')

		})
		function setformatday(obj) {
			var y = (obj.getFullYear()).toString();
			var m = (obj.getMonth() + 1).toString();
			if (m.length == 1) {
				m = "0" + m;
			}
			var d = obj.getDate().toString();
			if (d.length == 1) {
				d = "0" + d;
			}
			var time = y + "-" + m + "-" + d;
			return time;
		}
	</script>

</body>

