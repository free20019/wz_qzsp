		<script>
			$(function(){
				rebtnFhsyLeft();
				$(window).resize(function(){
					rebtnFhsyLeft();
				})
			});
			var rebtnFhsyLeft = function(){
				var winwidth = $(window).width();
				var bodywidth = $('body').width();
				var dybwidth = $('.infoPage').width();
				var fhsyBtn = $('#btnFhsy');
				if (winwidth > dybwidth){
					fhsyBtn.css({'right': ((bodywidth - dybwidth) / 2)});
				}else{
					fhsyBtn.css({'right':("winwidth")});
				}
			}
			$(window).scroll(function(){
				if($(this).scrollTop()>130){
					$("#gdxs").css("display","block");
				}else{
					$("#gdxs").css("display","none");
				}
			});
		</script>
