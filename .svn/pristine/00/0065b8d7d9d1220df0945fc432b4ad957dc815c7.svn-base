<script class="resize" type="text/javascript">
    $(window).resize(setInfoPageHeight);
    $('.infoPage').bind('DOMNodeInserted', setInfoPageHeight);
    setInfoPageHeight();
    $('#txtApplyType').text(appleTypeName($.cookie('get', { name: 'regsiterType' })));
