<script type="text/javascript">
	$('#dy').click(function() {
		var html = $('.printInfo').html();
		console.info(html);
		var dHtml = documentHtml(html);
		printHtml(dHtml);
	});
	 $.ajax({
        type:"post",
        url: basePath + "db/tbsh_cl",
        data:{
        },
        dataType: "json",
        success:function(data){
        	var type = data.info;
        	console.log(data)
        	if(type == '5'){
        		$("#zsxx").html('<p>审核中</p>');
        	}else if(type == '6'){
        		$("#zsxx").html('<p>恭喜您审核已通过，你的车可以成为网约车了</p>');
        	}else if(type == '7'){
        		$("#zsxx").html(data.yy);
        		$("#bczl_div").html('<button type="button" class="btn btn-info" id="go_bczl">重新补充资料</button>');
        		$("#go_bczl").click(function(){
        			$.cookie('set', { duration: .2, name: 'regsiterType', value: $.cookie('get', { name: 'regsiterType' })});
        			window.location.href= "shsp.html";
        		})
        	}else{
        		location.replace('../../../index.html');
        	}
        	$("#jdcsyr").text(data.data[0].XM);
    		$("#cphm").text(data.data[0].CPHM);
    		$("#sqsj").val(fomart_time1(data.data[0].SQRQ));
    		$("#clsbdh").text(data.data[0].VIN);
        },
        //调用出错执行的函数
        error: function(){
            //请求出错处理
        }
    });
</script>
