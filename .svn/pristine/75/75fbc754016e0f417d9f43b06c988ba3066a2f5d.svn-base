<script type="text/javascript">
	var ptf1 = false, ptf2 = false, ptf3 = false;
	$("#txsm_xyb").click(function(){
		var checkbox=document.getElementById('bwtk');
		if (false == ptf1 || false == ptf2 || false == ptf3) {
			Pop_up_box("请先阅读条款!", 2);
			return false;
		}
		if (!$("input[type='checkbox']").is(':checked')) {
			Pop_up_box("请确认本人已阅读并知悉内容!", 2);
			return false;
		} else if ($('#yhlx_select').val() == '0') {
			Pop_up_box("请选择用户类型!", 2);
			return false;
// 			window.location.href="index.html";
		} else {
            if ($('#yhlx_select').val() == '1') {
                $.cookie('set', { duration: .2, name: 'regsiterType', value: 'gr'});
            } else if ($('#yhlx_select').val() == '2') {
                $.cookie('set', { duration: .2, name: 'regsiterType', value: 'qy'});
            } else if ($('#yhlx_select').val() == '3') {
                $.cookie('set', { duration: .2, name: 'regsiterType', value: 'pt'});
            }
            window.location.href = "zctx.html";
		}
	});
	$('.panel-certificate').click(function() {
		var thisOne = $(this);
		var ptfFile = {};
		var title = '';
        var fileName = '';
		var url = '../../../resources/file/';
		switch ($(thisOne).attr('bt-c')) {
			case '1':
                title = '网络预约出租汽车经营服务管理暂行办法（交通运输部 工业和信息化部 公安部 商务部 工商总局 质检总局 国家网信办令2016年第60号）';
                fileName = '网络预约出租汽车经营服务管理暂行办法（交通运输部 工业和信息化部 公安部 商务部 工商总局 质检总局 国家网信办令2016年第60号）';
				ptf1 = true;
				break;
			case '2':
				title = '出租汽车驾驶员从业资格管理规定2016修订';
				fileName = '出租汽车驾驶员从业资格管理规定2016修订';
				ptf2 = true;
				break;
			case '3':
				title = '温州市网络预约出租汽车经营服务管理实施细则';
				fileName = '16温政办137（关于印发温州市网络预约出租汽车经营服务管理实施细则（试行）的通知）';
				ptf3 = true;
				break;
		}
		url += fileName + '.pdf';
		/*ptfFile = window.open(url, title);
		ptfFile.focus();*/
		layer.open({
			type: 2,
			title: title,
//			maxmin: true, //开启最大化最小化按钮
			area: ['893px', '600px'],
			content: url
		});
		thisOne.addClass('selected');
	});
	$('#bwtk').click(function() {
		if (!ptf1 || !ptf2 || !ptf3) {
			document.getElementById('bwtk').checked = false;
			return false;
		}
	});
</script>
