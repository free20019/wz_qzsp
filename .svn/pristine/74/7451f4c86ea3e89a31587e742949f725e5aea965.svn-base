    var defaultTextLayoutHtml = function(tagId, tagText, value) {
        var fontLength = 'fontLength9';
        if (tagText.length <= 9) { fontLength='fontLength9'; }
        else if (tagText.length <=12) { fontLength='fontLength12'; }
        var html = '<label class="' + fontLength + ' fontRight ml_5em" for="' + tagId + '">' + tagText + '：</label><span type="text" id="' + tagId + '" class="fontSize20" name="' + tagId.toUpperCase() + '">' + value + '</span>';
        return html;
    };
    var defaultInputTextLayoutHtml = function(tagId, tagText, tagVal){
        var html = '<label class="fontLength9 fontRight ml_5em" for="' + tagId + '">' + tagText + '：</label>' +
            '<input type="text" id="' + tagId + '" class="form-control" name="' + tagId.toUpperCase() + '" value="' + tagVal + '">';
        return html;
    };
    function getFormJson(){
        var o = {};
        var a = $("#clxxtx_1_form").serializeArray();
        $.each(a, function() {
            if (o[this.name] !== undefined) {
                if (!o[this.name].push) {
                    o[this.name] = [ o[this.name] ];
                }
                o[this.name].push(this.value || '');
            } else {
                o[this.name] = this.value || '';
            }
        });
        return o;
    }
