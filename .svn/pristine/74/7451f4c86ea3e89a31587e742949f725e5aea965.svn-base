// 				console.log(datap)
				$.ajax({
					type : "post",
					url : basePath + "clysz/clyszwnnr",
					data : {
						"postData" : datap
					},
					dataType : "json",
					success : function(data) {
						var data1 = data;
						if (data1.info == '提交成功') {
							layer.closeAll('loading');
							location.replace('ddfz.html');
						} else {
							layer.closeAll('loading');
