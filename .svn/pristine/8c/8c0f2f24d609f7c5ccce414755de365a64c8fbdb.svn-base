    var defaultTextLayoutHtml = function(tagId, tagText, value) {
        var fontLength = 'fontLength9';
        if (tagText.length <= 9) { fontLength='fontLength9'; }
        else if (tagText.length <=12) { fontLength='fontLength12'; }
        var html = '<label class="' + fontLength + ' fontRight ml_5em" for="' + tagId + '">' + tagText + '：</label><span type="text" id="' + tagId + '" class="fontSize20" name="' + tagId.toUpperCase() + '">' + value + '</span>';
        return html;
    };
    var defaultInputTextLayoutHtml = function(tagId, tagText, tagVal){
        var html = '<label class="fontLength9 fontRight ml_5em" for="' + tagId + '">' + tagText + '：</label>' +
            '<input type="text" id="' + tagId + '" class="form-control" name="' + tagId.toUpperCase() + '" value="' + tagVal + '">';
        return html;
    };
    function getFormJson(){
        var o = {};
        var a = $("#clxxtx_1_form").serializeArray();
        $.each(a, function() {
            if (o[this.name] !== undefined) {
                if (!o[this.name].push) {
                    o[this.name] = [ o[this.name] ];
                }
                o[this.name].push(this.value || '');
            } else {
                o[this.name] = this.value || '';
            }
        });
        return o;
    }
    /**
     * 短信界面提交后,由于图片form提交后天值传不过来,所以用这种onload方法获取后台的传值
     */
    var oFrm = document.getElementById("interface_data");
    oFrm.onload  = function() {
        if (this.readyState && this.readyState != 'complete') return;
        else{
            if (document.all){//IE
                var sub_con = document.frames["interface_data"].document;
            }else{//Firefox
                var sub_con = document.getElementById("interface_data").contentDocument;
            }
            sub_con = sub_con.body.innerHTML;  //取得返回值
            console.log(sub_con);
            if(sub_con.indexOf("OK")>=0){
				var dataPara = getFormJson("clxxtx_form");
				//个人申请type 1   平台代办  type2
				if('grcl' === dblx){
					dataPara.type = '2';
				}else{
					dataPara.type = '1';
				}
				dataPara.jdcsyr = $("#jdcsyr_q").text();
				dataPara.PDJL = $("#spddj").text();
				dataPara.PDJG = $("#pdjg").text();
				dataPara.PDYXQZ = $("#jsdjpdyxqz").text();
				var datap = JSON.stringify(dataPara);
				console.log(datap)
				$.ajax({
					type : "post",
					url : basePath + "clysz/clyszwnnr",
					data : {
						"postData" : datap
					},
					dataType : "json",
					success : function(data) {
						var data1 = data;
						if (data1.info == '提交成功') {
							layer.closeAll('loading');
							location.replace('shxx.html');
						} else {
							layer.closeAll('loading');
							Pop_up_box(data1.info, '2');
						}
					},
					error : function() {
						layer.closeAll('loading');
					}
				});
			}else{
                layer.closeAll('loading');
                Pop_up_box("提交失败",'2');
            }
        }
    };
