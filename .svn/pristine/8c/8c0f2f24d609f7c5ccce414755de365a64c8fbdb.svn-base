    //判断是否选择已经拥有车牌
	$(function(){
	    if(!$("#cph")[0].checked){
            $('.djzs').show();
            $('.hgzs').hide();
            $('.yzzs').hide();
            removeInputBorderStyle($('#sqclhp'));
            removeInputBorderStyle($('#jdczcrq'));
		}
        $("#cph").on("change",function(){
            if (!$("#cph")[0].checked){
                $('.djzs').show();
                $('.djzs input').click();
                $('.hgzs').hide();
                $('.yzzs').hide();
                $('#sqclhp').val('浙C');
                $("#zcrqwzc").text("");
                $("#sqclhp").removeAttr('readonly');
                $("#jdczcrq").removeAttr('readonly');
            }else{
                $('.djzs').hide();
                $('.hgzs').show();
                $(".hgzs input").click();
                $('.yzzs').show();
                $('#sqclhp').val('浙C#');
                $("#zcrqwzc").text("尚未注册");
                $("#jdczcrq").val('');
                $("#sqclhp").attr('readonly','true');
                $("#jdczcrq").attr('readonly','true');
                removeInputBorderStyle($('#sqclhp'));
                removeInputBorderStyle($('#jdczcrq'));
            }
        })
	})
	$.ajax({
		type : "post",
		url : basePath + "jsy/findbdbxx",
		data : {},
		dataType : "json",
		success : function(data) {
			console.log(data)
			if (data.length> 0) {
				if('企业' === $('#txtApplyType').text()){
					$("#jdcsyr_q").text(data[0].QYMC);
				}else{
					$("#jdcsyr_q").text(data[0].XM);
				}
				$("#yjhb_bc").val(data[0].USER_NAME);
			} else {
				layer.confirm(('您未登陆,请登录！'), {
					cancel : function(index) {
						window.location.href = basePath + "index.html";
						return false;
					},
					btn : [ '确定' ] //按钮
				}, function(index) {
					layer.close(index);
					window.location.href = basePath + "index.html";
				});
			}
		},
		error : function() {
		}
	});
</script>
