    function clxxtx_inputBoxReg() {
        var sqclhp = $('#sqclhp'),
            hdzkw = $('#hdzkw'),
            cllxyjg = $('#cllxyjg'),
            clzj = $('#clzj'),
            xhlc = $('#xhlc'),
            jgjs = $('#jgjs'),
            xllx = $('#xllx'),
            fdjh = $('#fdjh'),
			ppxh = $('#ppxh'),
			cpys = $('#cpys'),
			chec = $('#chec'),
			chek = $('#chek'),
			cheg = $('#cheg'),
			zzl = $('#zzl'),
			spddj = $('#spddj'),
			pdjg = $('#pdjg'),
			jdczcrq = $('#jdczcrq');
        //车牌号码
        if (sqclhp.val() == "" || !regLicenseplate1.test(sqclhp.val())) {
            submitRegErrorInputBorderStyle(sqclhp);
        }
        //核定载客位
        if (hdzkw.val() == "" || !regHdzkw.test(hdzkw.val())) {
            submitRegErrorInputBorderStyle(hdzkw);
        }
        if(cllxyjg.val()=='新能源'){
            if (!regXnyc.test(clzj.val()) && !regXhlc.test(xhlc.val()) && !regJgyjs1.test(jgjs.val())){
                submitRegErrorInputBorderStyle(clzj);
                submitRegErrorInputBorderStyle(xhlc);
                submitRegErrorInputBorderStyle(jgjs);
            }
        }else{
            if (!regXnyc.test(clzj.val()) || (!regJgyjs.test(jgjs.val()) && !regJgyjs1.test(jgjs.val()))){
                submitRegErrorInputBorderStyle(clzj);
                submitRegErrorInputBorderStyle(jgjs);
            }
        }
        //车辆类型
        if (xllx.val() == "" || !regName_zw.test(xllx.val())) {
            submitRegErrorInputBorderStyle(xllx);
        }
        //发动机号
        if (fdjh.val() == "" || !regFdjh.test(fdjh.val())) {
            submitRegErrorInputBorderStyle(fdjh);
        }
        //车辆厂牌
        if (ppxh.val() == "" || !regPpxh.test(ppxh.val())) {
            submitRegErrorInputBorderStyle(ppxh);
        }
        //车辆颜色
        if (cpys.val() == "" || !regName_zw.test(cpys.val())) {
            submitRegErrorInputBorderStyle(cpys);
        }
        //车长
        if (chec.val() == "" || !regCkg.test(chec.val())) {
            submitRegErrorInputBorderStyle(chec);
        }
        //车宽
        if (chek.val() == "" || !regCkg.test(chek.val())) {
            submitRegErrorInputBorderStyle(chek);
        }
        //车高
        if (cheg.val() == "" || !regCkg.test(cheg.val())) {
            submitRegErrorInputBorderStyle(cheg);
        }
        //总质量
        if (zzl.val() == "" || !regZzlzl.test(zzl.val())) {
            submitRegErrorInputBorderStyle(zzl);
        }
        //技术评定结论
        if (spddj.text() == "" || !regName_zw.test(spddj.text())) {
            submitRegErrorInputBorderStyle(spddj);
